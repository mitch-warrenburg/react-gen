{
  "name": "@react-gen/cgen",
  "version": "1.0.0",
  "description": "Generate boilerplate for React-based projects.",
  "author": "Mitch Warrenburg",
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/mitch-warrenburg/cgen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitch-warrenburg/cgen.git"
  },
  "bugs": {
    "url": "https://github.com/mitch-warrenburg/cgen/issues"
  },
  "bin": {
    "react-cli": "./dist/react-cli.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "12"
  },
  "keywords": [
    "react",
    "generate",
    "workflow",
    "storybook",
    "boilerplate"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint && git add ."
    }
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "immutable": "^4.0.0-rc.12",
    "shelljs": "^0.8.4",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/chalk": "^2.2.0",
    "@types/immutable": "^3.8.7",
    "@types/node": "^14.6.0",
    "@types/pug": "^2.0.4",
    "@types/shelljs": "^0.8.8",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "chalk-cli": "^4.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "prettier": "^2.1.1",
    "pug": "^3.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.8",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-node-dev": "^1.0.0-pre.60",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "npm run clean && rollup -c && cp dist/index.js .",
    "clean": "rimraf dist coverage",
    "lint": "eslint 'packages/src/**/*.{js,ts}'",
    "format": "prettier --write 'packages/src/**/*.{js,ts}'"
  }
}
